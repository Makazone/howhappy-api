{
  "name": "howhappy",
  "version": "1.0.0",
  "description": "Survey collection with audio transcription and analysis",
  "type": "module",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "build": "tsc",
    "lint": "eslint . && prettier --check .",
    "format": "prettier --write .",
    "test": "vitest",
    "test:unit": "vitest unit",
    "test:integration": "vitest integration",
    "worker": "tsx watch src/worker/index.ts",
    "typecheck": "tsc --noEmit",
    "prisma:generate": "prisma generate",
    "prisma:migrate:dev": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:studio": "prisma studio"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.729.0",
    "@aws-sdk/s3-request-presigner": "^3.893.0",
    "@prisma/client": "^6.1.0",
    "bcryptjs": "^3.0.0",
    "express": "^5.0.1",
    "jsonwebtoken": "^9.0.2",
    "pg-boss": "^10.1.7",
    "pino": "^9.5.0",
    "pino-pretty": "^13.0.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.18.0",
    "@types/express": "^5.0.1",
    "@types/jsonwebtoken": "^9.0.8",
    "@types/node": "^22.10.6",
    "@types/supertest": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^8.20.0",
    "@typescript-eslint/parser": "^8.20.0",
    "eslint": "^9.18.0",
    "prettier": "^3.5.0",
    "prisma": "^6.1.0",
    "supertest": "^7.0.0",
    "testcontainers": "^10.15.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
